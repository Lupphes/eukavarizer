k8s {
    namespace = 'eukavarizer'
    runAsUser = 1000
    computeResourceType = 'Job'
    launchDir = '/mnt/data1'
    workDir = '/mnt/data1/tmp'
}

executor {
    queueSize = 15
}

process {
    executor = 'k8s'
    pod = [[securityContext:[fsGroupChangePolicy:'OnRootMismatch', runAsUser:1000, runAsGroup:1, fsGroup:1]], [automountServiceAccountToken:false]]
}
